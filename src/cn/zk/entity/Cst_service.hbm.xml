<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.zk.entity">
	<class name="Cst_service" table="cst_service">
		<id name="svr_id" type="string" length="20">
			<generator class="assigned"></generator>
		</id>
		<property name="svr_type" type="string" length="20"></property>
		<property name="svr_title" type="string" length="500"></property>
		<property name="svr_cust_name" type="string" length="17"></property>
		<property name="svr_status" type="string" length="10"></property>
		<property name="svr_request" type="string" length="500"></property>
		<property name="svr_create_by" type="string" length="50"></property>
		<property name="svr_create_date" type="string"></property>
		<property name="svr_due_to" type="string" length="50"></property>
		<property name="svr_due_date" type="string"></property>
		<property name="svr_deal" type="string" length="500"></property>
		<property name="svr_deal_date" type="string"></property>
		<property name="svr_result" type="string" length="500"></property>
		<property name="svr_satisfy" type="integer"></property>
	</class>
</hibernate-mapping>