<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping package="cn.zk.basd">
    	<class name="Dict" table="dict_tab">
    		<id name="dict_id" type="integer" length="20">
    			<generator class="increment"></generator>
    		</id>
    		<property name="dict_type" type="string" length="50" not-null="true"></property>
    		<property name="dict_item" type="string" length="50" not-null="true"></property>
    		<property name="dict_value" type="string" length="50" not-null="true"></property>
    		<property name="dict_is_editable" type="string" ></property>
    		
    	</class>
  </hibernate-mapping>

